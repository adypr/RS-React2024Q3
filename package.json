{
  "name": "my-app2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:fix": "prettier . --write",
    "prepare": "husky install",
    "test": "vitest --coverage",
    "clean": "rd /s /q node_modules dist"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npx eslint .",
    "*.{js,jsx,ts,tsx,html,css,scss}": "npx prettier . --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^9.1.2",
    "sass": "^1.77.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "husky": "^9.1.4",
    "jsdom": "^24.1.1",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "redux-mock-store": "^1.5.4",
    "typescript": "^5",
    "vitest": "^2.0.5"
  }
}
